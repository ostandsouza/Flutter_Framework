<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Flutter Mobile Automation "  verbose="1" preserve-order="true" thread-count="1"  parallel="false">
<listeners>
    <listener class-name="com.codecraft.flutter.testNG.EmailableTestNGReport"></listener>
    <listener class-name="com.codecraft.flutter.testNG.TestListener"></listener>

</listeners>

    <parameter name="environment" value="IphoneLocal"/>
    <!--<parameter name="environment" value="IPhone14BS_16V"/>-->

    <test name="FLUTTER-1: Regression" preserve-order="true" parallel="false" enabled ="true">
